{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30,
      "includeFiles": "node_modules/.prisma/**"
    }
  },
  "env": {
    "PRISMA_CLIENT_ENGINE_TYPE": "library"
  },
  "regions": ["hkg1"]
}